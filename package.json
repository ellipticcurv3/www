    "@fortawesome/vue-fontawesome": "3.0.0-1",
    "@types/luxon": "^1.26.2",
    "@vue/component-compiler-utils": "^3.2.0",
    "@vuepress/plugin-debug": "^2.0.0-alpha.6",
    "@vuepress/plugin-pwa-popup": "^2.0.0-alpha.8",
