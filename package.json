  "scripts": {
    "start": "yarn dev",
    "test": "yarn reset && yarn build && yarn clean",
    "dev": "vuepress dev src",
    "build": "vuepress build src",
    "serve": "serve dist/",
    "deploy": "rsync -ru dist/. ricardobalk.nl:/var/www/ricardobalk.nl --delete",
